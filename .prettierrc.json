{
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "printWidth": 80,
  "plugins": ["prettier-plugin-tailwindcss"]
}
